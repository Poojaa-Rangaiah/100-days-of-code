//Write a program to identify of the a number is positive or negative.
#include <iostream>
using namespace std;
int main () {
    int a;
    cout<<"Enter a number : ";
    cin>>a;
    if (a < 0)
    cout<<"Negative";
    else if (a > 0)
    cout<<"Positive";
    else
    cout<<"Neither positive nor negative";
    return 0;
}
