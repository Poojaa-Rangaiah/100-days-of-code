//Write a program to find the Quadrants in which coordinates lie.
#include <iostream>
using namespace std;
int main () {
    int x, y;
    cout<<"Enter x and y coordinates : ";
    cin>>x>>y;
    if (x > 0 && y > 0)
    cout<<"This point lies in first quadrant.";
    else if ( x < 0 && y > 0)
    cout<<"This point lies in second quadrant.";
    else if (x < 0 && y < 0)
    cout<<"This point lies in Third quadrant.";
    else if (x > 0 && y < 0)
    cout<<"This point lies in fourth quadrant.";
    else if (x == 0 && y != 0)
    cout<<"This point lies on y axis.";
    else if (x != 0 && y == 0)
    cout<<"This point lies on x axis.";
    else
    cout<<"This point lies in origin.";
    return 0;
}
