//Write a program to find Fibonacci series up to n.
#include <iostream>
using namespace std;

int main() {
 int n, a = 0, b = 1, c;
 cout<<"Enter a num upto which Fibonacci series must be printed : ";
 cin>>n;
 cout<<"The Fibonacci series are: "<<endl;
 cout<<a<<","<<b;
 for(int i = 1; i <= n-2; i++) {
    c = a + b;
    cout<<","<<c;
    a = b;
    b = c;
 }
 cout<<".";
 return 0;
}
