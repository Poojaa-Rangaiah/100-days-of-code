//Write a program to identify if the character is a vowel or consonant.
# include <iostream>
using namespace std;
int main () {
    char a;
    cout<<"Enter a character : ";
    cin>>a;
    if (a == 'a' || a == 'e' || a == 'i' || a == 'o' || a == 'u' || a == 'A' || a == 'E' || a == 'I' || a == 'O' || a == 'U') {
        cout<<"Vowel";
    }
    else if (!((a >= 'a' && a <= 'z') || (a >= 'A' && a <= 'Z'))) {
        cout<<"Invalid Input";
    }
    else {
        cout<<"Consonant";
    }
    return 0;
}
