//Write a program to reverse a given number.
#include <iostream>
using namespace std;
int main() {
  int n, sum = 0, rem;
  cout<<"Enter a number : ";
  cin>>n;
  while(n != 0) {
    rem = n%10;
    sum = sum*10 + rem;
    n = n/10;
  }
  cout<<"The reversed digit is given as : "<<sum;
  return 0;
}
